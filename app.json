{
  "expo": {
    "name": "InterviewAI Mobile",
    "slug": "interviewai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/interviewai-logo.png",
    "scheme": "interviewai",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/interviewai-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.interviewai.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/interviewai-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.interviewai.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/interviewai-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b71a3cd7-0788-4758-ada2-642c0ea25808"
      }
    },
    "owner": "tabrezdn1"
  }
}
